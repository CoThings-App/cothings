<h1>Show Room</h1>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @room.name %>
  </li>

  <li>
    <strong>Count:</strong>
    <%= @room.count %>
  </li>

  <li>
    <strong>Capacity:</strong>
    <%= @room.capacity %>
  </li>

  <li>
    <strong>Group:</strong>
    <%= @room.group %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.room_path(@conn, :edit, @room) %></span>
<span><%= link "Back", to: Routes.room_path(@conn, :index) %></span>
